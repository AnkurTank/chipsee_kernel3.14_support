		mmc2 {
		    compatible = "ti,omap4-hsmmc";
		    vmmc-supply = <&vmmcsd_fixed>;
		    pinctrl-names = "default";
		    pinctrl-0 = <&emmc_pins>;
		    bus-width = <8>;
		    ti,non-removable;
		    
		    reset = <&rstctl 0 0>;
		    reset-names = "eMMC_RSTn-CONSUMER";
		    
		    ti,vcc-aux-disable-is-sleep;
		    status = "okay";
	    };*/

	    
	  
	};
	
	
};
